{
  "dateStart": "2019",
  "description": "React wrapper for Admin LTE CSS Framework",
  "keywords": [
    "react",
    "reactjs",
    "adminlte",
    "adminl te"
  ],
  "bugs": {
    "url": "https://github.com/Zanoshky/admin-lte-react/issues"
  },
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zanoshky/admin-lte-react.git"
  },
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "admin-lte-react",
  "author": "Marko Zanoski",
  "module": "dist/admin-lte-react.esm.js",
  "size-limit": [
    {
      "path": "dist/admin-lte-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/admin-lte-react.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2",
    "admin-lte": "^3.1.0",
    "bootstrap": "~4.6.0",
    "chart.js": "~2.9.4",
    "chartjs-plugin-datalabels": "^1.0.0",
    "moment": "^2.29.1",
    "popper.js": "^1.16.1",
    "react-chartjs-2": "~2.11.2"
  }
}
